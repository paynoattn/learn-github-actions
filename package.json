{
  "name": "learn-github-actions",
  "version": "1.0.0",
  "description": "learn github actions",
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha -r ts-node/register src/**/*.spec.ts --exit",
    "test:coverage": "nyc npm test",
    "build": "tsc",
    "dev": "tsc -w",
    "start": "node dist/server.js",
    "lint": "eslint --cache",
    "pretty": "prettier --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paynoattn/learn-github-actions.git"
  },
  "keywords": [
    "learn",
    "github",
    "actions"
  ],
  "author": "Chris Pawlukiewicz <hi@paynoattn.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/paynoattn/learn-github-actions/issues"
  },
  "homepage": "https://github.com/paynoattn/learn-github-actions#readme",
  "dependencies": {
    "express": "^4.17.3",
    "request-ip": "^2.1.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/express": "^4.17.7",
    "@types/mocha": "^8.0.1",
    "@types/node": "^14.0.27",
    "@types/request-ip": "^0.0.35",
    "@types/sinon": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/eslint-plugin-tslint": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "mocha": "^8.1.0",
    "nyc": "^15.1.0",
    "sinon": "^9.0.2",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.3",
    "typescript": "^3.9.7"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.spec.ts",
      "src/**/index.ts"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "lcov",
      "html",
      "text",
      "json"
    ],
    "sourceMap": true,
    "instrument": true,
    "reportDir": "coverage"
  }
}
